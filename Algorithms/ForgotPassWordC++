int indio,attempts = 1;
int main(){
string formato;
cin>>indio;
string names[indio]; 
for(int i = 0; i<indio; i++){
    cin>>names[i];
}
while(cin>>formato){
    int aux[formato.size()];
    for(int i = 0; i<formato.size();i++){
        if(formato.at(i)== 'X'){
            attempts*=indio;
        }else{
            attempts*=10;
        }
    }
    for(int i = 0; i<attempts; i++){
        for(int j = 0; j<formato.size(); j++){
            if(j==formato.size()-1){
                       if(formato.at(j) == 'X'){
                           cout<<names[aux[j]%indio]<<endl;
                       }else{
                           cout<<aux[j]%10<<endl;
                       }
                   }else{
                    if(formato.at(j) == 'X'){
                        cout<<names[aux[j]% indio];
                    }else{
                        cout<<aux[j]%10;
                    }
                   }
        }
        for(int j = formato.size()-1; j>=0; j--){
                   aux[j]++;
                   if(formato.at(j)== 'X'){
                    if(aux[j]% indio !=0){
                        break; 
                    }
                   }else{
                       if(aux[j]%10 !=0){
                           break;
                       }
                   }
               }
               attempts = 1;
    }
}

    return 0;
}
