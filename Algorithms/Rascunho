#include <bits/stdc++.h>
using namespace std;
int lines, x, y, aux;
string aux;
bool control;


int main(){
    cin>> lines;
    pair <int,int> objects[lines];
    int answers[lines];
    for(int i = 0; i<lines; i++){
        cin>>x>>y;
        objects[i] = {x,y};
    }
    for(int i = 0; i<lines; i++){
            if(i==0){
                x = objects[1].first;
                y = objects[1].second;
            }else{
                x = objects[0].first;
                y = objects[0].second;
            }
    	for(int j = 0; j<lines;j++){ 
    	if(j!=i){
            if(x>objects[j].first){
                x = objects[j].first;
            }
            if(y<objects[j].second){
                y = objects[j].second
            }
        }
    	
    	
    	}
        answers[i] = y-x;
    }
    
    
    
    return 0;
}
/*
4   - lines
1 3 - x / y
2 6
0 4
3 3

1
----code---
3 - 1 = 2
3 - 2 = 1
3 - 2 = 1
3 - 3 = 0
desconsider 0, print 1
*/
